{
  "version": "1.0.0",
  "nodes": [
    {
      "id": "demo-create-appointment",
      "type": "POST_REQUEST",
      "position": { "x": 100, "y": 100 },
      "data": {
        "url": "https://api.example.com/appointments",
        "headers": {
          "Content-Type": "application/json"
        },
        "bodyType": "json",
        "body": "{\n  \"patientName\": \"John Doe\",\n  \"appointmentDate\": \"2024-01-15\",\n  \"appointmentTime\": \"10:00 AM\"\n}"
      },
      "connections": [],
      "outputs": [
        {
          "name": "appointmentId",
          "description": "Created appointment ID",
          "type": "string",
          "source": "response.body.id",
          "example": "APT-12345"
        }
      ],
      "inputs": []
    },
    {
      "id": "demo-extract-confirmation",
      "type": "VARIABLE_EXTRACTOR",
      "position": { "x": 100, "y": 250 },
      "data": {
        "variableName": "confirmationNumber",
        "jsonPath": "$.confirmationNumber",
        "defaultValue": "N/A"
      },
      "connections": [],
      "outputs": [
        {
          "name": "confirmationNumber",
          "description": "Extracted confirmation number",
          "type": "string",
          "source": "confirmationNumber",
          "example": "CONF-789"
        }
      ],
      "inputs": [
        {
          "name": "response",
          "description": "Response data to extract from",
          "type": "object",
          "required": true
        }
      ]
    },
    {
      "id": "demo-get-appointment-details",
      "type": "GET_REQUEST",
      "position": { "x": 100, "y": 400 },
      "data": {
        "url": "https://api.example.com/appointments/#(confirmationNumber)",
        "headers": {
          "Accept": "application/json"
        },
        "queryParams": {},
        "timeout": 30000
      },
      "connections": [],
      "outputs": [
        {
          "name": "appointmentDetails",
          "description": "Appointment details",
          "type": "object",
          "source": "response.body",
          "example": {}
        }
      ],
      "inputs": [
        {
          "name": "confirmationNumber",
          "description": "Confirmation number from previous step",
          "type": "string",
          "required": true
        }
      ]
    },
    {
      "id": "demo-status-check",
      "type": "STATUS_ASSERTION",
      "position": { "x": 50, "y": 550 },
      "data": {
        "expectedStatus": 200,
        "operator": "equals"
      },
      "connections": [],
      "outputs": [],
      "inputs": []
    }
  ],
  "connections": [
    {
      "id": "conn-1",
      "fromNodeId": "demo-create-appointment",
      "toNodeId": "demo-extract-confirmation",
      "fromOutput": "response",
      "toInput": "response",
      "label": "Extract Confirmation"
    },
    {
      "id": "conn-2",
      "fromNodeId": "demo-extract-confirmation",
      "toNodeId": "demo-get-appointment-details",
      "fromOutput": "confirmationNumber",
      "toInput": "confirmationNumber",
      "label": "Use Confirmation"
    },
    {
      "id": "conn-3",
      "fromNodeId": "demo-get-appointment-details",
      "toNodeId": "demo-status-check",
      "fromOutput": "status",
      "toInput": "status",
      "label": "Validate Status"
    }
  ],
  "exportedAt": "2024-01-01T00:00:00.000Z"
}
